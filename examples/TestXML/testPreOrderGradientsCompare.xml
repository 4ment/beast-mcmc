<?xml version="1.0" standalone="yes"?>
<beast>

    <!--start to scale up-->
    <!-- The list of taxa analyse (can also include dates/ages).                 -->
    <!-- ntax=17                                                                 -->
    <taxa id="taxaLong">
        <taxon id="D4Brazi82">
            <date value="1982.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4ElSal83">
            <date value="1983.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4ElSal94">
            <date value="1994.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Indon76">
            <date value="1976.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Indon77">
            <date value="1977.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Mexico84">
            <date value="1984.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4NewCal81">
            <date value="1981.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Philip64">
            <date value="1964.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Philip56">
            <date value="1956.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Philip84">
            <date value="1984.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4PRico86">
            <date value="1986.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4SLanka78">
            <date value="1978.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Tahiti79">
            <date value="1979.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Tahiti85">
            <date value="1985.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Thai63">
            <date value="1963.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Thai78">
            <date value="1978.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="D4Thai84">
            <date value="1984.0" direction="forwards" units="years"/>
        </taxon>
    </taxa>
    <!-- The sequence alignment (each sequence refers to a taxon above).         -->
    <!-- ntax=17 nchar=1485                                                      -->
    <alignment id="alignmentLong" dataType="nucleotide">
        <sequence>
            <taxon idref="D4Brazi82"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4ElSal83"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4ElSal94"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Indon76"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Indon77"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Mexico84"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4NewCal81"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Philip64"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Philip56"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Philip84"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4PRico86"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4SLanka78"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Tahiti79"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Tahiti85"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Thai63"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Thai78"/>
            
        </sequence>
        <sequence>
            <taxon idref="D4Thai84"/>
            
        </sequence>
    </alignment>

    <!-- The unique patterns for all positions                                   -->
    <!-- npatterns=138                                                           -->
    <patterns id="patterns" from="1">
        <alignment idref="alignmentLong"/>
    </patterns>

    <!-- A prior assumption that the population size has remained constant       -->
    <!-- throughout the time spanned by the genealogy.                           -->
    <constantSize id="constant" units="years">
        <populationSize>
            <parameter id="constant.popSize" value="380.0" lower="0.0" upper="38000.0"/>
        </populationSize>
    </constantSize>

    <!-- Generate a random starting tree under the coalescent process            -->
    <coalescentTree id="startingTreeLong">
        <taxa idref="taxaLong"/>
        <constantSize idref="constant"/>
    </coalescentTree>

    <report>
        <coalescentTree idref="startingTreeLong"/>
    </report>


    <!--<newick id="startingTreeLong">-->
        <!--(((D4ElSal94:25.81403421468474,D4Indon76:7.814034214684739):244.56614874230695,(D4Brazi82:36.80326223293307,D4ElSal83:37.80326223293307):221.57692072405862):29.481672726408988,D4Indon77:282.8618556834007);-->
    <!--</newick>-->

    <treeModel id="treeModelLong">
        <coalescentTree idref="startingTreeLong"/>
        <rootHeight>
            <parameter id="treeModelLong.rootHeight"/>
        </rootHeight>
        <nodeHeights internalNodes="true">
            <parameter id="treeModelLong.internalNodeHeights"/>
        </nodeHeights>
        <nodeHeights internalNodes="true" rootNode="true">
            <parameter id="treeModelLong.allInternalNodeHeights"/>
        </nodeHeights>
    </treeModel>

    <arbitraryBranchRates id="branchRatesLong">
        <rates>
            <parameter id="strict.rates" value="1.0" lower="0"/>
        </rates>
        <treeModel idref="treeModelLong"/>
    </arbitraryBranchRates>

    <!-- HKY -->

    <hkyModel id="hky85">
        <frequencies>
            <frequencyModel id='frequencies1' dataType="nucleotide">
                <frequencies>
                    <parameter dimension="4" value="0.1 0.3 0.2 0.4"/>
                </frequencies>
            </frequencyModel>
        </frequencies>
        <kappa>
            <parameter id="hky85.kappa" value="1.0" lower="0.0" upper="100.0"/>
        </kappa>
    </hkyModel>

    <!-- site model                                                              -->
    <siteModel id="siteModel">
        <substitutionModel>
            <hkyModel idref="hky85"/>
        </substitutionModel>
        <gammaShape gammaCategories="2">
            <parameter id="siteModel.alpha" value="0.5" lower="0.0"/>
        </gammaShape>
    </siteModel>

    <!-- Likelihood for tree given sequence data                                 -->
    <treeDataLikelihood id="treeLikelihoodLong" useAmbiguities="false" usePreOrder="true" scalingScheme="always" delayScaling="false">
        <patterns idref="patterns"/>
        <treeModel idref="treeModelLong"/>
        <siteModel idref="siteModel"/>
        <arbitraryBranchRates idref="branchRatesLong"/>
    </treeDataLikelihood>

    <report>
        HKY85:
        ln L =
        <treeDataLikelihood idref="treeLikelihoodLong"/>
    </report>

    <branchRateGradient id="branchRateGradientDiscreteLong" traitName="Sequence">
        <treeDataLikelihood idref="treeLikelihoodLong"/>
    </branchRateGradient>

    <report>
        <branchRateGradient idref="branchRateGradientDiscreteLong"/>
    </report>

    <distributionLikelihood id="ratesPrior">
        <data>
            <parameter idref="strict.rates"/>
        </data>
        <distribution>
            <logNormalDistributionModel meanInRealSpace="true">
                <mean>
                    <parameter id="diffusion.mean" value="1E-3" lower="0.0"/>
                </mean>
                <stdev>
                    <parameter id="diffusion.stdev" value="1.0" lower="0.0"/>
                </stdev>
            </logNormalDistributionModel>
        </distribution>
    </distributionLikelihood>

    <!--Reference: testBranchGradientFull.xml in Alex Fisher's hmc_random_walk/xml/ folder-->
    <operators id="operators">

        <scaleOperator scaleFactor="0.75" weight="2">
                <parameter idref="strict.rates"/>
        </scaleOperator>

        <scaleOperator scaleFactor="0.75" weight="1">
            <parameter idref="diffusion.mean"/>
        </scaleOperator>

        <scaleOperator scaleFactor="0.75" weight="1">
            <parameter idref="siteModel.alpha"/>
        </scaleOperator>


        <!--
             <scaleOperator weight="1" scaleFactor="0.75">
                 <parameter idref="actual.rates"/>
             </scaleOperator>
         -->
    </operators>

    <mcmc id="mcmc" chainLength="10000000">
        <posterior id="posterior">
            <prior id="prior">
                <distributionLikelihood idref="ratesPrior"/>
                <exponentialPrior offset="0" mean="1">
                	<parameter idref="siteModel.alpha"/>
                </exponentialPrior>
                <exponentialPrior offset="0" mean="1">
                	<parameter idref="diffusion.mean"/>
                </exponentialPrior>
            </prior>
            <!--0105-->
            <likelihood id="likelihood">
                <treeDataLikelihood idref="treeLikelihoodLong"/>
            </likelihood>
            <!-- 0105 -->

        </posterior>
        <operators idref="operators"/>
        <log logEvery="10">
            <posterior idref="posterior"/>
            <!--<branchRateGradient idref="branchRateGradientDiscreteLong"/>-->
<!--
                        <traitLogger traitName="derivative.Sequence" nodes="all">
                            <treeDataLikelihood idref="treeLikelihoodLong"/>
                            <treeModel idref="treeModelLong"/>
                        </traitLogger>
 -->
        </log>
        <logTree logEvery="100" nexusFormat="true" fileName="testPreOrderGradients.trees">
            <treeDataLikelihood idref="treeLikelihoodLong"/>
            <treeModel idref="treeModelLong"/>
        </logTree>
        <log logEvery="100" fileName="testPreOrderGradientsCompare.log">
            <parameter idref="strict.rates"/>
            <parameter idref="diffusion.mean"/>
            <parameter idref="siteModel.alpha"/>
        </log>
    </mcmc>

    <traceAnalysis fileName="testPreOrderGradientsCompare.log"/>

    <report>
        <property name="timer">
            <object idref="mcmc"/>
        </property>
    </report>
</beast>

